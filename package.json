  },
  "homepage": "https://github.com/flowcommerce/node-sdk#readme",
  "devDependencies": {
    "@flowio/babel-preset-flowio": "1.1.0",
    "@flowio/eslint-config-flowio": "9.0.0",
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.2.3",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.6.1",
    "eslint-plugin-jsx-a11y": "5.1.0",
    "eslint-plugin-react": "7.1.0",
